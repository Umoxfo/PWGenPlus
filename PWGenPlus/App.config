        </sectionGroup>
    </configSections>
    <userSettings>
        <PWGenPlus.Properties.Settings>
            <setting name="Characters" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="PasswordLength" serializeAs="String">
                <value>12</value>
            </setting>
            <setting name="UpperCase" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="LowerCase" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="Numbers" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="Symbols" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="CustomCharSet" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="CustomCharacters" serializeAs="String">
                <value />
